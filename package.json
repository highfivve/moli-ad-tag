{
  "name": "@highfivve/ad-tag",
  "version": "5.0.0-alpha.3",
  "license": "Apache-2.0",
  "description": "An ad tag implementation called moli",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -p tsconfig.build.json --watch",
    "clean": "rm -rf lib",
    "setup": "npm run clean && npm ci && npm run codeGen && tsc --composite false",
    "start": "npm run watch",
    "watch": "tsc --composite false --watch",
    "compile": "tsc --noEmit",
    "lint": "eslint ./ad-tag/source --ext .ts,.tsx && npm run prettier",
    "lint:fix": "eslint ./ad-tag/source --ext .ts,.tsx --fix && npm run prettier --write",
    "test": "mocha --require ts-node/register ad-tag/**/*.test.ts",
    "test:junit": "mocha --forbid-only --reporter mocha-junit-reporter --reporter-options mochaFile=./temp/test-results.xml --require ts-node/register source/**/*.test.ts",
    "prettier": "prettier --config .prettierrc --ignore-path .prettierignore --check \"ad-tag/source/**/*.ts*\"",
    "validate": "npm run compile && npm run lint && npm run test",
    "codeGen": "node codeGen.js",
    "schema": "ts-json-schema-generator --path 'ad-tag/source/ts/types/*.ts' --type 'MoliConfig' --out schema.json",
    "release": "np --no-release-draft --no-publish",
    "github:publish": "npm run clean && npm run build && npm publish --registry https://npm.pkg.github.com/",
    "postpublish": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:highfivve/moli-ad-tag.git",
    "directory": "ad-tag"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "node": ">=18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@types/chai": "4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/domready": "^1.0.0",
    "@types/jsdom": "^20.0.0",
    "@types/mocha": "8.2.2",
    "@types/node": "^20.10.4",
    "@types/sinon": "10.0.0",
    "@types/sinon-chai": "3.2.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "babel-loader": "^8.2.2",
    "chai": "4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^34.8.1",
    "eslint-plugin-preact": "^0.1.0",
    "jsdom": "19.0.0",
    "mocha": "8.4.0",
    "mocha-junit-reporter": "2.0.0",
    "np": "^10.0.5",
    "prettier": "^3.2.5",
    "sinon": "10.0.0",
    "sinon-chai": "3.6.0",
    "ts-json-schema-generator": "^2.2.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1"
  }
}
