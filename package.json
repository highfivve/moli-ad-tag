{
  "name": "moli-ad-tag",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "description": "An ad tag implementation called moli",
  "main": "source/ts/index.ts",
  "scripts": {
    "start": "yarn watch",
    "watch": "yarn compile:ts:watch",
    "compile": "yarn compile:ts",
    "compile:ts": "tsc --noEmit",
    "compile:ts:watch": "tsc --noEmit --watch",
    "lint": "yarn lint:ts",
    "lint:ts": "tslint --project tsconfig.json",
    "lint:ts:fix": "tslint --project tsconfig.json --fix",
    "test": "echo 'no tests yet!'",
    "build:examples": "rm -rf dist && mkdir dist && yarn build:example:self-contained",
    "build:example:self-contained": "mkdir -p dist/self-contained && cd examples/self-contained && yarn install && yarn build && cp dist/* ../../dist/self-contained"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.gutefrage.net:7999/gd/moli-ad-tag.git"
  },
  "engines": {
    "node": ">=8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@types/domready": "^1.0.0",
    "babel-loader": "^8.0.2",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^3.1.4",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.1"
  },
  "dependencies": {
    "domready": "^1.0.8"
  }
}
