{
  "name": "@highfivve/ad-tag",
  "version": "1.20.1",
  "license": "UNLICENSED",
  "description": "An ad tag implementation called moli",
  "main": "index.ts",
  "scripts": {
    "start": "yarn watch",
    "watch": "yarn compile:ts:watch",
    "compile": "yarn compile:ts",
    "compile:ts": "tsc --noEmit",
    "compile:ts:watch": "tsc --noEmit --watch",
    "lint": "yarn lint:ts",
    "lint:ts": "tslint --project tsconfig.json",
    "lint:ts:fix": "tslint --project ../tsconfig.json --fix",
    "test": "mocha --require ts-node/register tests/**/*.test.ts",
    "test:junit": "mocha --forbid-only --reporter mocha-junit-reporter --reporter-options mochaFile=./temp/test-results.xml --require ts-node/register tests/**/*.test.ts",
    "docs": "yarn typedoc --readme source/ts/types/README.md --theme default --out docs/ source/ts/types/",
    "validate": "yarn compile && yarn lint && yarn test && yarn docs"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.gutefrage.net:7999/gd/moli-ad-tag.git"
  },
  "engines": {
    "node": ">=8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@types/chai": "^4.1.7",
    "@types/domready": "^1.0.0",
    "@types/jsdom": "^12.2.4",
    "@types/mocha": "^5.2.5",
    "@types/sinon": "^5.0.5",
    "@types/sinon-chai": "^3.2.0",
    "babel-loader": "^8.0.2",
    "chai": "^4.2.0",
    "jsdom": "^15.1.1",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.0",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.1"
  },
  "dependencies": {}
}
