{
  "name": "self-contained",
  "version": "1.0.0",
  "description": "Publisher boostrap template",
  "main": "index.ts",
  "scripts": {
    "postinstall": "yarn link moli-ad-tag",
    "build": "yarn clean && yarn build:prebid && yarn build:tag",
    "build:tag": "webpack --mode=production",
    "build:prebid": "cd node_modules/prebid.js && npm install && node_modules/.bin/gulp build --modules=../../prebid/modules.json",
    "start": "yarn build:prebid && webpack-dev-server --mode=development",
    "link:debug": "ln -sf ../../moli-debugger/dist/ dist",
    "unlink": "yarn unlink moli-ad-tag",
    "clean": "rm -rf dist/"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.gutefrage.net:7999/gd/publisher-tags.git"
  },
  "keywords": [
    "publisher"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "moli-ad-tag": "file:../.."
  },
  "devDependencies": {
    "html-webpack-plugin": "^3.2.0",
    "prebid.js": "1.28.0",
    "ts-loader": "^5.2.2",
    "typescript": "^3.1.3",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
