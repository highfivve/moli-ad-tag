{
  "name": "@highfivve/moli-release",
  "version": "1.0.0",
  "description": "Moli release tools",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:prod": "yarn clean && yarn compile --mode=production",
    "start": "yarn watch",
    "watch": "yarn compile:ts:watch",
    "compile": "yarn compile:ts",
    "compile:ts": "webpack",
    "compile:ts:watch": "yarn compile:ts --watch",
    "lint": "tslint --project tsconfig.json && yarn prettier",
    "clean": "rm -rf lib/**",
    "validate": "yarn prettier && yarn compile",
    "prettier": "prettier --config ../.prettierrc --check \"**/*.ts*\"",
    "prettier:fix": "yarn prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.gutefrage.net:7999/gd/moli-ad-tag.git"
  },
  "keywords": [
    "release"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "commander": "^6.2.0",
    "dir-compare": "2.3.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.3",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
