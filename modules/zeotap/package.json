{
  "name": "@highfivve/module-zeotap",
  "version": "3.29.3",
  "license": "Apache-2.0",
  "description": "Zeotap integration",
  "main": "./lib/zeotap.js",
  "typings": "./lib/zeotap.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "setup": "yarn",
    "make:nodemodule": "tsc -p tsconfig.build.json",
    "compile": "tsc --noEmit",
    "lint": "yarn lint:ts",
    "lint:ts": "eslint ./ --ext .ts,.tsx && yarn prettier",
    "lint:ts:fix": "eslint ./ --ext .ts,.tsx --fix && yarn prettier --write",
    "test": "mocha --require ts-node/register zeotap.test.ts",
    "test:junit": "mocha --forbid-only --reporter mocha-junit-reporter --reporter-options mochaFile=./temp/test-results.xml --require ts-node/register zeotap.test.ts",
    "prettier": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore --check \"**/*.ts*\"",
    "validate": "yarn compile && yarn lint && yarn test",
    "validate:github": "yarn compile && yarn lint && yarn test:junit",
    "validate:jenkins": "yarn compile && yarn lint && yarn test:junit"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:highfivve/moli-ad-tag.git",
    "directory": "modules/zeotap"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "node": ">=8.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/jsdom": "16.2.5",
    "@types/mocha": "8.0.4",
    "@types/sinon": "9.0.9",
    "@types/sinon-chai": "3.2.5",
    "chai": "4.2.0",
    "jsdom": "16.4.0",
    "mocha": "8.2.1",
    "mocha-junit-reporter": "2.0.0",
    "sinon": "9.2.1",
    "sinon-chai": "3.5.0",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@highfivve/ad-tag": "^3.29.3"
  }
}
