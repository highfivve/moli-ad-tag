{
  "name": "@highfivve/module-justpremium-skin",
  "version": "1.20.1",
  "license": "UNLICENSED",
  "description": "Blocks ad slots when a wallpaper is being delivered",
  "main": "index.ts",
  "scripts": {
    "compile": "tsc --noEmit",
    "lint": "yarn lint:ts",
    "lint:ts": "tslint --project tsconfig.json && yarn prettier",
    "lint:ts:fix": "tslint --project tsconfig.json --fix && yarn prettier --write",
    "test": "export TS_NODE_TRANSPILE_ONLY=true && mocha --require ts-node/register index.test.ts",
    "test:junit": "export TS_NODE_TRANSPILE_ONLY=true && mocha --forbid-only --reporter mocha-junit-reporter --reporter-options mochaFile=./temp/test-results.xml --require ts-node/register index.test.ts",
    "prettier": "prettier --config ../../.prettierrc --check \"**/*.ts*\"",
    "validate": "yarn compile && yarn lint && yarn test",
    "validate:jenkins": "yarn compile && yarn lint && yarn test:junit"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.gutefrage.net:7999/gd/moli-ad-tag.git"
  },
  "engines": {
    "node": ">=8.0"
  },
  "devDependencies": {
    "@highfivve/ad-tag": "*",
    "@types/chai": "4.2.13",
    "@types/jsdom": "12.2.4",
    "@types/mocha": "8.0.3",
    "@types/sinon": "9.0.8",
    "@types/sinon-chai": "3.2.5",
    "chai": "4.2.0",
    "jsdom": "15.1.1",
    "mocha": "8.1.3",
    "mocha-junit-reporter": "2.0.0",
    "sinon": "9.2.0",
    "sinon-chai": "3.5.0",
    "ts-loader": "^8.0.5",
    "ts-node": "9.0.0",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {}
}
